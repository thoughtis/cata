{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 2,
	"customTemplates": [],
	"settings": {
		"appearanceTools": true,
		"custom": {
			"fontScale": "max( 1rem, calc( 0.9375rem + 0.3125vw ) )",
			"rootPadding": "3.75vw"
		},
		"layout": {
			"contentSize": "calc( 32 * var( --wp--custom--font-scale, 1rem ) )",
			"wideSize": "calc( 64 * var( --wp--custom--font-scale, 1rem ) )"
		},
		"color": {
			"custom": false,
			"customDuotone": false,
			"customGradient": false,
			"duotone": [],
			"gradients": [],
			"defaultPalette": false,
			"defaultGradients": false,
			"palette": []
		},
		"spacing": {
			"units": [ "em", "rem", "px", "%", "ch", "vw" ],
			"spacingSizes": [
				{
					"slug": "20",
					"name": "XXS",
					"size": "0.375em"
				},
				{
					"slug": "30",
					"name": "XS",
					"size": "0.75em"
				},
				{
					"slug": "40",
					"name": "SM",
					"size": "1.125em"
				},
				{
					"slug": "50",
					"name": "MD",
					"size": "1.5em"
				},
				{
					"slug": "60",
					"name": "LG",
					"size": "2.25em"
				},
				{
					"slug": "70",
					"name": "XL",
					"size": "3em"
				},
				{
					"slug": "80",
					"name": "XXL",
					"size": "4.5em"
				}
			]
		},
		"typography": {
			"dropCap": false,
			"fontSizes": [
				{
					"name": "Extra Small",
					"size": "0.75em",
					"slug": "extra-small"
				},
				{
					"name": "Small",
					"size": "0.875em",
					"slug": "small"
				},
				{
					"name": "Medium ( H5 )",
					"size": "1.125em",
					"slug": "medium"
				},
				{
					"name": "Large ( H3 )",
					"size": "1.375em",
					"slug": "large"
				},
				{
					"name": "Extra Large ( H1 )",
					"size": "2em",
					"slug": "x-large"
				}
			]
		}
	},
	"styles": {
		"spacing": {
			"blockGap": "1.5em"
		},
		"typography": {
			"fontSize": "var( --wp--custom--font-scale, 1rem )"
		}
	},
	"templateParts": []
}
