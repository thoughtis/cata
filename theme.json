{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 2,
	"customTemplates": [],
	"settings": {
		"appearanceTools": true,
		"useRootPaddingAwareAlignments": true,
		"custom": {
			"fontScale": "max( 1rem, calc( 0.9375rem + 0.3125vw ) )",
			"rootPadding": "3.75vw"
		},
		"layout": {
			"contentSize": "calc( 32 * var( --wp--custom--font-scale, 1rem ) )",
			"wideSize": "calc( 64 * var( --wp--custom--font-scale, 1rem ) )"
		},
		"color": {
			"custom": false,
			"customDuotone": false,
			"customGradient": false,
			"duotone": [],
			"gradients": [],
			"defaultPalette": false,
			"defaultGradients": false,
			"palette": []
		},
		"spacing": {
			"units": [ "em", "rem", "px", "%", "ch", "vw" ],
			"spacingSizes": [
				{
					"slug": "20",
					"name": "XXS",
					"size": "0.375em"
				},
				{
					"slug": "30",
					"name": "XS",
					"size": "0.75em"
				},
				{
					"slug": "40",
					"name": "SM",
					"size": "1.125em"
				},
				{
					"slug": "50",
					"name": "MD",
					"size": "1.5em"
				},
				{
					"slug": "60",
					"name": "LG",
					"size": "2.25em"
				},
				{
					"slug": "70",
					"name": "XL",
					"size": "3em"
				},
				{
					"slug": "80",
					"name": "XXL",
					"size": "4.5em"
				}
			]
		},
		"typography": {
			"dropCap": false,
			"fontSizes": [
				{
					"name": "Extra Small",
					"size": "0.75em",
					"slug": "extra-small"
				},
				{
					"name": "Small",
					"size": "0.875em",
					"slug": "small"
				},
				{
					"name": "Medium ( H5 )",
					"size": "1.125em",
					"slug": "medium"
				},
				{
					"name": "Large ( H3 )",
					"size": "1.375em",
					"slug": "large"
				},
				{
					"name": "Extra Large ( H1 )",
					"size": "2em",
					"slug": "x-large"
				}
			]
		}
	},
	"styles": {
		"spacing": {
			"blockGap": "calc( var( --wp--style--block-gap, 1.5em ) * 2 )",
			"padding": {
				"top": "1.6667em",
				"left": "3.75vw",
				"bottom": "2.7778em",
				"right": "3.75vw"
			}
		},
		"typography": {
			"fontSize": "var( --wp--custom--font-scale, 1rem )"
		},
		"elements": {
			"button": {
				"border": {
					"width": "0.125em",
					"radius": "var(--wp--custom--border-radius, 0.1875em)",
					"color": "transparent",
					"style": "solid"
				},
				"spacing": {
					"padding": {
						"top": "var(--wp--preset--spacing--20, 0.375em)",
						"bottom": "var(--wp--preset--spacing--20, 0.375em)",
						"left": "var(--wp--preset--spacing--30, 0.75em)",
						"right": "var(--wp--preset--spacing--30, 0.75em)"
					}
				}
			}
		},
		"blocks": {
			"cata/faq": {
				"elements": {
					"heading": {
						"typography": {
							"fontSize": "var( --wp--custom--font-scale, 1rem )"
						}
					}
				}
			},
			"core/button": {
				"variations": {
					"outline" : {
						"border": {
							"width": {
								"ref": "styles.elements.button.border.width"
							},
							"radius": {
								"ref": "styles.elements.button.border.radius"
							}
						},
						"spacing": {
							"padding": {
								"top": {
									"ref": "styles.elements.button.spacing.padding.top"
								},
								"bottom": {
									"ref": "styles.elements.button.spacing.padding.bottom"
								},
								"left": {
									"ref": "styles.elements.button.spacing.padding.left"
								},
								"right": {
									"ref": "styles.elements.button.spacing.padding.right"
								}
							}
						}
					}
				}
			},
			"core/pullquote": {
				"spacing": {
					"padding": {
						"top": "var(--wp--preset--spacing--50,1.5em)",
						"bottom": "var(--wp--preset--spacing--50,1.5em)",
						"left": "calc(var(--wp--custom--root-padding,3.75vw)/ 2)",
						"right": "calc(var(--wp--custom--root-padding,3.75vw)/ 2)"
					}
				}
			}
		}
	},
	"templateParts": []
}
